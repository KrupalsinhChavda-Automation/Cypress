  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.3.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-xpath": "^2.0.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "cypress-downloadfile": "^1.2.4",
    "mocha-allure-reporter": "^1.4.0"
  }
